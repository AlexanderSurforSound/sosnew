    },
    "./operations": {
      "import": "./dist/modules/operations/index.js",
      "types": "./dist/modules/operations/index.d.ts"
    },
    "./analytics": {
      "import": "./dist/modules/analytics/index.js",
      "types": "./dist/modules/analytics/index.d.ts"
    },
